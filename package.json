{
  "name": "BaroPlate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "generate-assets": "node ./generateAssets.js",
    "postinstall": "node ./bin/postInstall",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "ad": "yarn rnuc .env.dev && npx react-native run-android",
    "as": "yarn rnuc .env.st && npx react-native run-android",
    "ap": "yarn rnuc .env.prd && npx react-native run-android",
    "id": "yarn rnuc .env.dev && npx react-native run-ios --simulator \"iPhone 15 Pro Max\"",
    "is": "yarn rnuc .env.st && npx react-native run-ios --simulator \"iPhone 15 Pro Max\"",
    "ip": "yarn rnuc .env.prd && npx react-native run-ios --simulator \"iPhone 15 Pro Max\"",
    "abd": "cd android && yarn rnuc .env.dev && ./gradlew bundleRelease",
    "abs": "cd android && yarn rnuc .env.st && ./gradlew bundleRelease",
    "abp": "cd android && yarn rnuc .env.prd && ./gradlew bundleRelease",
    "convert:bundle": "cd android/app/build/outputs/bundle/release && bundletool build-apks --mode universal --bundle ./app-release.aab --output ./app.apks && mv app.apks app.zip && unzip app.zip && echo /android/app/build/outputs/bundle/release/universal.apk"
  },
  "dependencies": {
    "@baont97/rn-loader": "0.1.0",
    "@eslint/compat": "1.1.0",
    "@eslint/js": "9.5.0",
    "@react-navigation/bottom-tabs": "7.1.3",
    "@react-navigation/elements": "2.2.5",
    "@react-navigation/native": "7.0.14",
    "@react-navigation/native-stack": "7.2.0",
    "@reduxjs/toolkit": "2.2.2",
    "axios": "1.6.8",
    "date-fns": "3.6.0",
    "eslint-plugin-react": "7.34.2",
    "formik": "^2.4.6",
    "globals": "15.5.0",
    "i18next": "23.10.1",
    "jwt-decode": "4.0.0",
    "lodash.isequal": "4.5.0",
    "lottie-react-native": "7.2.2",
    "react": "18.3.1",
    "react-i18next": "14.1.0",
    "react-native": "0.75.5",
    "react-native-biometrics": "3.0.1",
    "react-native-bootsplash": "5.4.1",
    "react-native-date-picker": "5.0.10",
    "react-native-device-info": "11.1.0",
    "react-native-encrypted-storage": "4.0.3",
    "react-native-gesture-handler": "2.23.1",
    "react-native-international-phone-number": "0.9.1",
    "react-native-keychain": "8.2.0",
    "react-native-mmkv": "2.12.2",
    "react-native-reanimated": "3.16.7",
    "react-native-rename": "3.2.14",
    "react-native-safe-area-context": "5.2.0",
    "react-native-screens": "4.6.0",
    "react-native-ultimate-config": "6.0.1",
    "react-redux": "9.1.0",
    "redux-logger": "3.0.6",
    "redux-persist": "6.0.0",
    "typescript-eslint": "7.13.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/cli": "^14.0.0",
    "@react-native-community/cli-platform-android": "^14.0.0",
    "@react-native-community/cli-platform-ios": "^14.0.0",
    "@react-native/babel-preset": "^0.75.0",
    "@react-native/eslint-config": "0.74.84",
    "@react-native/metro-config": "^0.75.0",
    "@react-native/typescript-config": "0.74.84",
    "@rnx-kit/align-deps": "^3.0.1",
    "@types/lodash.isequal": "^4.5.8",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "@types/redux-logger": "^3.0.13",
    "axios-logger": "^2.8.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "chalk": "^5.3.0",
    "dateformat": "^5.0.3",
    "eslint": "9.x",
    "jest": "^29.2.1",
    "path": "^0.12.7",
    "prettier": "2.8.8",
    "react-test-renderer": "^18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@1.22.19",
  "rnx-kit": {
    "kitType": "app",
    "alignDeps": {
      "requirements": [
        "react-native@0.75"
      ],
      "capabilities": [
        "animation",
        "babel-preset-react-native",
        "core",
        "core-android",
        "core-ios",
        "core/metro-config",
        "gestures",
        "jest",
        "navigation/native",
        "react",
        "react-test-renderer",
        "safe-area",
        "screens"
      ]
    }
  }
}
